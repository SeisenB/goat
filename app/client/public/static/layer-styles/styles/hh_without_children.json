{
  "name": "hh_without_children",
  "rules": [
    {
      "name": "Zero",
      "symbolizers": [
        {
          "kind": "Fill",
          "color": "#ffffff",
          "outlineColor": "#004B83",
          "outlineWidth": 0,
          "outlineDasharray": []
        }
      ],
      "filter": 
        ["==", "score", 0]
    },
	{
      "name": "Below 1000",
      "symbolizers": [
        {
          "kind": "Fill",
          "color": "#D5EDFF",
          "outlineColor": "#004B83",
          "outlineWidth": 0,
          "outlineDasharray": []
        }
      ],
      "filter": 
        ["==", "score", 1]
    },
    {
      "name": "1000 to 1999",
      "symbolizers": [
        {
          "kind": "Fill",
          "color": "#2CA5FF",
          "outlineColor": "#004B83",
          "outlineWidth": 0,
          "outlineDasharray": []
        }
      ],
      "filter": 
		["==", "score", 2]
    },
    {
      "name": "2000 to 2999",
      "symbolizers": [
        {
          "kind": "Fill",
          "color": "#006FC2",
          "outlineColor": "#004B83",
          "outlineWidth": 0,
          "outlineDasharray": []
        }
      ],
      "filter":
		["==", "score", 3]
    },
    {
      "name": "3000 and over",
      "symbolizers": [
        {
          "kind": "Fill",
          "color": "#004B83",
          "outlineColor": "#004B83",
          "outlineWidth": 0,
          "outlineDasharray": []
        }
      ],
      "filter": 
        ["==", "score", 4]
    },
	{
      "name": "MissingData",
      "symbolizers": [
        {
          "kind": "Fill",
          "color": "#000000",
          "outlineColor": "#004B83",
          "outlineWidth": 0,
          "outlineDasharray": []
        }
      ],
      "filter": 
        ["==", "score", "nodata"]
	}
  ]
}
